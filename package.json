{
  "name": "@thecodeaware/ts-interface-builder",
  "version": "0.0.7",
  "license": "MIT",
  "description": "Builder pattern for typescript interfaces.",
  "main": "dist/index.js",
  "typings": "dist/index",
  "homepage": "https://github.com/thecodeaware/ts-interface-builder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/thecodeaware/ts-interface-builder"
  },
  "bugs": {
    "url": "https://github.com/thecodeaware/ts-interface-builder/issues"
  },
  "keywords": [
    "typescript",
    "interface",
    "builder",
    "pattern",
    "proxy"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "jest",
    "clean": "rm -rf dist/*",
    "prepare": "npm run test && npm run clean && npm run build"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "ghooks": "2.0.4",
    "jest": "23.5.0",
    "ts-jest": "^23.1.3",
    "tslint": "5.11.0",
    "typescript": ">=2.7.2"
  }
}
